@model IEnumerable<Docimax.Interface_ICD.Model.CodeOrderModel>

@{
    ViewBag.Title = "待抢单列表";
}

<h2>@ViewBag.Title</h2>

<table class="table">
    <tr>
        <th>
            @Html.DisplayName("订单号")
        </th>
        <th>
            @Html.DisplayName("创建时间")
        </th>
        <th>
            @Html.DisplayName("订单所属医院")
        </th>
        <th></th>
    </tr>

    @foreach (var item in Model)
    {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.PlatformOrderCode)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.CreateTime)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.ORGName)
            </td>

            <td>
                @Html.ActionLink("Edit", "领单", new { /* id=item.PrimaryKey */ })
            </td>
        </tr>
    }

</table>
